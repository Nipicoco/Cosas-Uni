Algoritmo Compra
	
	Escribir "Ingrese numero zapatillas"
	
	Leer n_zapatillas
	
	Escribir "Ingrese numero jeans"
	
	Leer n_jeans
	
	Escribir "Ingrese numero poleras"
	
	Leer n_poleras
	
	total_compra<-n_zapatillas*12000+n_jeans*15000+n_poleras*10000
	
	Escribir "El total de la compra es:",total_compra
	
FinAlgoritmo

